{
  "name": "imamsudarajat04/laravel-base-service-repo",
  "description": "A Laravel package for Service and Repository pattern with modern PHP 8.3 features",
  "keywords": [
    "laravel",
    "service",
    "repository",
    "pattern",
    "php8.3",
    "crud",
    "eloquent"
  ],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Imam Sudarajat",
      "email": "imamsudarajat04@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/console": "^10.0|^11|^12",
    "illuminate/database": "^10.0|^11|^12"
  },
  "autoload": {
    "psr-4": {
      "Imamsudarajat04\\LaravelBaseServiceRepo\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Imamsudarajat04\\LaravelBaseServiceRepo\\Providers\\LaravelBaseServiceRepoServiceProvider"
      ],
      "aliases": {
        "ServiceRepo": "Imamsudarajat04\\LaravelBaseServiceRepo\\Facades\\ServiceRepo"
      }
    }
  },
  "prefer-stable": true,
  "minimum-stability": "stable"
}
